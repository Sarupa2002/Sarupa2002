
 import java.util.*;
 class Program1
 {
     public static void main(String Args[])
     {
         Scanner sc=new Scanner(System.in);
         int n,i,j;
         n=sc.nextInt();
         for(i=1;i<=n;i++)
         {
             for(j=1;j<=i;j++)
             {
                 System.out.print("*");
             }
             System.out.print("\n");
         }
     }
 }
 
